{
  "include": ["src"],
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@types": ["../types"],
    },
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}
