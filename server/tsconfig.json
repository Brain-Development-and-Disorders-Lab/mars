{
  "include": ["src", "types"],
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}
