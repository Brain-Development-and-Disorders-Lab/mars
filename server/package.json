{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Henry Burgess <henryjburg@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "esbuild src/index.ts --platform=node --outdir=dist --bundle --minify",
    "buildDev": "esbuild src/index.ts --platform=node --outdir=dist --bundle --sourcemap --watch",
    "buildDebug": "esbuild src/index.ts --platform=node --outdir=dist --bundle --sourcemap",
    "startDev": "node --inspect dist/index.js",
    "test": "jest --run-in-band",
    "clean": "rm -rf dist",
    "lint": "prettier --write ."
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@types/graphql-upload": "^16.0.7",
    "axios": "^1.6.0",
    "consola": "^3.0.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.1",
    "esbuild": "^0.17.14",
    "express": "^4.19.2",
    "express-fileupload": "^1.4.0",
    "graphql": "^16.8.1",
    "graphql-upload": "^16.0.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.1",
    "jwks-rsa": "^3.0.1",
    "lodash": "^4.17.21",
    "mongodb": "^4.17.0",
    "mquery": "^5.0.0",
    "nanoid": "^4.0.2",
    "papaparse": "^5.4.1",
    "source-map-support": "^0.5.21",
    "tmp": "^0.2.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.4.1",
    "@types/lodash": "^4.14.192",
    "@types/node": "^20.3.1",
    "@types/papaparse": "^5.3.7",
    "@types/react": "^18.0.25",
    "@types/tmp": "^0.2.3",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.7.0",
    "prettier": "2.6.2",
    "semver": "^7.5.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.6.3",
    "word-wrap": "^1.2.4"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "node_modules/(?!(nanoid))"
    ]
  }
}
